# Server
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/fasttrack?schema=public

# External Carrier API
CARRIER_API_URL=http://localhost:3001
CARRIER_API_TIMEOUT=5000

# Sync Configuration
SYNC_CRON_SCHEDULE=*/5 * * * *
SYNC_ENABLED=true

# Retry Configuration
RETRY_MAX_ATTEMPTS=3
RETRY_BASE_DELAY=1000
RETRY_MAX_DELAY=10000

# Logging
LOG_LEVEL=debug
